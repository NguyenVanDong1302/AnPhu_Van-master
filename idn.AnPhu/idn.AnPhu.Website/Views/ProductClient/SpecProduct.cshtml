
<style>
.specification {
margin: 15px auto 0;
overflow: auto;
height: 312px;
font-size: 9pt;
}
.spec_table {
width: 100%;
height: auto;
position: relative;
overflow: hidden;
clear: both;
border-top: 1px solid #575A5D;
}
.specification .spec_table {
border-top: none;
}
.specification .tablescroll_head {
border-top: solid 1px #D8D9D9;
border-right: solid 1px #D8D9D9;
}
.spec_table table {
    width: 100%;
    table-layout: fixed;
    empty-cells: show;
    background-color:White;
}
.spec_table table th {
font-size: 13px;
color: #333;
line-height: 20px;
font-weight: bold;
padding: 8px 0 11px 0;
text-align: center;
border: 1px solid #D8D9D9;
border-right: 0;
border-top: 0;
border-image: initial;
}
.spec_table table th.type {
background: #E7E7E6;
color: #57585A;
font-size: 13px;
font-weight: bold;
border-bottom: 1px solid #D8D9D9;
border-left: 0 !important;
}
.spec_table table td {
font-size: 13px;
color: #666;
line-height: 20px;
font-weight: normal;
padding: 8px 0 11px 0;
text-align: center;
border: 1px solid #D8D9D9;
border-right: 0;
border-top: 0;
border-image: initial;
}
</style>
<div class="row" style="margin:0 0 70px 0; max-width:1000px">
    <div class="col-lg-12">
<div class="specification">
	@Html.Raw(ViewBag.contentFile)
</div>
        </div>
    </div>
